{
    "version": "1.4.1",
    "type": "woocommerce-plugin",
    "require": {
        "php-http/curl-client": "^1.7",
        "php-http/client-common": "^1.9",
        "guzzlehttp/psr7": "^1.6",
        "php-http/logger-plugin": "^1.1",
        "monolog/monolog": "^1.25"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform": {
            "php": "7.4"
        },
        "secure-http": true,
        "sort-packages": true
    }
}
